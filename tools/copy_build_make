cp ../src/c/* ../uhd/host/examples/
cp ../CMakeLists.txt ../uhd/host/examples/
cd ../uhd/host/build
CC=/usr/bin/clang CXX=/usr/bin/clang++ \
      cmake -DPYTHON_EXECUTABLE=/opt/local/bin/python3.9 \
     -DPYTHON_INCLUDE_DIR=/opt/local/Library/Frameworks/Python.framework/Versions/3.9/Headers \
     -DPYTHON_LIBRARY=/opt/local/Library/Frameworks/Python.framework/Versions/3.9/Python ../ \
     -DRUNTIME_PYTHON_EXECUTABLE=/opt/local/bin/python3.9
make
